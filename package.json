{
  "name": "adonis-start",
  "version": "2.0.0",
  "adonis-version": "4.1.0",
  "description": "lffg's boilerplate for AdonisJs.",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "dev": "concurrently --kill-others -p \"[{name}]\" -n \"SERVER,ASSETS\" -c \"bgGreen.bold,bgMagenta.bold\" \"npm run serve:dev\" \"npm run build:dev\"",
    "serve": "./node_modules/.bin/adonis serve",
    "serve:dev": "npm run serve -- --dev",
    "build": "./node_modules/.bin/gulp build",
    "build:dev": "./node_modules/.bin/gulp dev",
    "lint": "node_modules/.bin/eslint **/*.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app",
    "adonis start",
    "gulp",
    "boilerplate"
  ],
  "author": "lffg",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.12",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/lucid-slugify": "^1.0.3",
    "@adonisjs/mail": "^3.0.9",
    "@adonisjs/session": "^1.0.28",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.0.6",
    "fa-icon-list": "^1.0.0",
    "got": "^9.2.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "sanitize-html": "^1.19.1",
    "shortid": "^2.2.13",
    "sprintf-js": "^1.1.1",
    "yiq": "^1.0.0"
  },
  "devDependencies": {
    "@adonisjs/cli": "^4.0.9",
    "concurrently": "^4.0.1",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "node-sass": "^4.10.0",
    "sqlite3": "^4.0.4"
  },
  "autoload": {
    "App": "./app"
  }
}

{
  "extends": ["lffg"],
  "globals": {
    "use": true
  },
  "overrides": [
    {
      "files": "resources/**/*.js",
      "parserOptions": {
        "sourceType": "script"
      },
      "env": {
        "jquery": true
      },
      "rules": {
        "strict": ["error", "function"],
        "no-var": "off"
      }
    }
  ]
}
